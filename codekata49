function solution(n) {
    const MOD = 1234567;
    if (n === 1) return 1;
    if (n === 2) return 2;
    
    let dp = new Array(n + 1).fill(0);
    dp[0] = 1; // There is 1 way to stay at the starting point
    dp[1] = 1; // There is 1 way to reach the first square
    
    for (let i = 2; i <= n; i++) {
        dp[i] = (dp[i-1] + dp[i-2]) % MOD;
    }
    
    return dp[n];
}
