function solution(absolutes, signs) {
    let sum = 0;
    for (let i = 0; i < absolutes.length; i++) {
        if (signs[i]) {
            sum += absolutes[i]; // if sign is true, add absolute value
        } else {
            sum -= absolutes[i]; // if sign is false, subtract absolute value
        }
    }
    return sum;
}
