function solution(absolutes, signs) {
    let sum = 0;
    for (let i = 0; i < absolutes.length; i++) {
        if (signs[i]) {
            sum += absolutes[i]; // if sign is true, add absolute value
        } else {
            sum -= absolutes[i]; // if sign is false, subtract absolute value
        }
    }
    return sum;
}

function solution(phone_number) {
    let length = phone_number.length;
    let obscured = '*'.repeat(length - 4) + phone_number.substring(length - 4);
    return obscured;
}

function solution(numbers) {
    let sum = 0;
    for (let i = 0; i <= 9; i++) {
        if (!numbers.includes(i)) {
            sum += i;
        }
    }
    return sum;
}
